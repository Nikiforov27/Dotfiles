unbind-key all
borderwidth 0
color activeborder "#e0def4"
color inactiveborder "#191724"
color menubg "#191724"
color menufg "#e0def4"
color font "#e0def4"
color selfont "#191724"
bind-key 4-p "dmenu_run -fn 'DroidSansMNerdFontMono-10'" 
bind-key 4-m "flameshot gui "
bind-key 4-b "qutebrowser"
bind-key 4-y "telegram-desktop"
bind-key 4-t window-vtile
bind-key 4-q window-close
gap 0 0 0 0   
bind-mouse 4-3 window-resize
bind-key C-q quit
bind-mouse 4-1 window-move
bind-key 4-f window-maximize
bind-key 4-Return "xterm"
bind-key 4-1            group-only-1
bind-key 4-2            group-only-2
bind-key 4-3            group-only-3
bind-key 4-0		group-toggle-all
bind-key 4M-k		window-move-up-big
bind-key 4M-j   	window-move-down-big
bind-key 4M-h		window-move-left-big
bind-key 4M-l	        window-move-right-big
bind-key 4-n		window-cycle
bind-key 4-e		window-rcycle
sticky yes
